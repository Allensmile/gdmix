          "gradleScalaTest": "gradle.plugin.com.github.maiflai:gradle-scalatest:0.24",
          "shadow": "com.github.jengelman.gradle.plugins:shadow:2.0.4",
          "ligradle-python": {
            "libraries": [
              "ligradle-python"
            ],
            "version": "1.*"
          }
        },
        "version": "5.6.4"
      }
    },
    "type": "gradle",
    "versions": {
      "java": "1.8.0_121",
      "scala": "2.11.8"
    }
  },
  "runtime": {},
  "review": {},
